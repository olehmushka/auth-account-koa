{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "import-name": {
      "severity": "off"
    },
    "semicolon": [
      true,
      "always"
    ],
    "variable-name": {
      "options": [
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    }
  },
  "rulesDirectory": []
}